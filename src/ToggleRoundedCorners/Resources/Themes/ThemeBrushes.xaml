   <SolidColorBrush x:Key="WindowBackgroundSecondaryColor" Color="#50000000" />
   <SolidColorBrush x:Key="WindowForegroundColor" Color="#c8d6e5" />
   
   <SolidColorBrush x:Key="GripColor" Color="#30CCCCCC" />
   <SolidColorBrush x:Key="SeparatorColor" Color="#30CCCCCC" />
   <SolidColorBrush x:Key="BorderColor" Color="#10CCCCCC" />
